    {
        xMult = -1;
    }
	
    if (ud)
    {
        yMult = -1;
    }

	Result[int2(xPos + (15 * xMult), yPos)] = color;
	Result[int2(xPos + (16 * xMult), yPos)] = color;
	Result[int2(xPos + (15 * xMult), yPos - (1 * yMult))] = color;
	Result[int2(xPos + (13 * xMult), yPos - (2 * yMult))] = color;
	Result[int2(xPos + (14 * xMult), yPos - (2 * yMult))] = color;
	Result[int2(xPos + (15 * xMult), yPos - (2 * yMult))] = color;
	Result[int2(xPos + (13 * xMult), yPos - (18 * yMult))] = color;
	Result[int2(xPos + (14 * xMult), yPos - (18 * yMult))] = color;
	Result[int2(xPos + (13 * xMult), yPos - (19 * yMult))] = color;
	Result[int2(xPos + (14 * xMult), yPos - (19 * yMult))] = color;
